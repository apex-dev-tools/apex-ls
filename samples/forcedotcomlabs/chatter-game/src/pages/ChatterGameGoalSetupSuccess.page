<apex:page title="Goal Setup Successfully">
    <div class="message confirmM3">
        <table border="0" cellpadding="0" cellspacing="0" class="messageTable" style="padding:0px;margin:0px;">
            <tr valign="top">
                <td>
                    <img alt="Error" class="msgIcon" src="/s.gif" title="Error"/>
                </td>
                <td class="messageCell">
                    <div id="goalSetupSuccessMessage" class="messageText">
                        Your Goal was successfully created.        
                     </div>
                </td>
            </tr>
        </table>

        <p>&nbsp;</p>
        
        <h1>Next Steps</h1>

        <p><apex:outputLink value="{!$Page.ChatterGameGoalSetupWizard}">Create another Goal</apex:outputLink>.</p>
    </div>
</apex:page>