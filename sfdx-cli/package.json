{
  "name": "apexlink",
  "version": "0.8.0",
  "author": "Kevin Jones <nawforce@gmail.com> (https://github.com/nawforce)",
  "bugs": "https://github.com/nawforce/apexlink/issues",
  "description": "SFDX CLI plugin for Apexlink",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@salesforce/command": "^1.4.1",
    "@salesforce/core": "^1.3.2",
    "@types/cross-spawn": "^6.0.0",
    "locate-java-home": "1.1.2",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "1.4.1",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^13",
    "ts-node": "^8",
    "tslint": "^5",
    "typescript": "~3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/jars",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/nawforce/apexlink",
  "keywords": [
    "sfdx-plugin"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "apexlink": {
        "description": "Run Apex code analysis"
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "nawforce/apexlink",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
