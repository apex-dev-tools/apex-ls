{
  "name": "apexlink",
  "version": "0.5",
  "author": "Kevin Jones <nawforce@gmail.com> (https://github.com/nawforce)",
  "bugs": "https://github.com/nawforce/apexlink/issues",
  "description": "SFDX CLI plugin for Apexlink",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/errors": "^1",
    "@salesforce/command": "^2.1.0",
    "@salesforce/core": "^2.1.3",
    "cross-spawn": "^7.0.1",
    "gitignore-parser": "^0.0.2",
    "java": "^0.11.1",
    "moment": "^2.24.0",
    "tmp": "^0.1.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/plugin-help": "^2",
    "@oclif/test": "^1",
    "@salesforce/dev-config": "1.5.0",
    "@types/chai": "^4",
    "@types/java": "^0.9.0",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^13",
    "ts-node": "^8",
    "tslint": "^5",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/jars",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/nawforce/apexlink",
  "keywords": [
    "sfdx-plugin"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "apexlink": {
        "description": "Run Apex code analysis"
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "nawforce/apexlink",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": ""
  }
}
